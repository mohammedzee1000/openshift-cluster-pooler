{
    "Name": "minishift-simple",
    "Description": "minishift-simple",
    "Size": 2,
    "MaxSize": 2,
    "UnusedClusterTimeout": "1h",
    "UsedClusterTimeout": "1h",
    "ParallelProvisioning": 1,
    "ParallelDeProvisioning": 1,
    "Provision-command": "minishift-simple-provision.sh",
    "DeProvisionCommand": "minishift-simple-deprovision.sh",
    "ForceDeProvisionCommand": "minishift-simple-deprovision.sh",
    "ClusterUrlCommand": "minishift-simple-cluster-url.sh",
    "ClusterAdminUserCommand": "echo 'developer'",
    "ClusterAdminPasswordCommand": "echo 'developer'",
    "ClusterCAFilePath": "$HOME/.minishift/profiles/$CLUSTER_UUID/certs/ca.pem",
    "ClusterCertFilePath": "$HOME/.minishift/profiles/$CLUSTER_UUID/certs/cert.pem",
    "ClusterKeyFilePath": "cat $HOME/.minishift/profiles/$CLUSTER_UUID/certs/key.pem",
    "ClusterExtraInfoCommand": "echo ''"
}
